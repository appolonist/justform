{
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-console": "off"
    },
    "extends": "eslint:recommended",
    "parser" : "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },

    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true,
        "node": true
    }
}
